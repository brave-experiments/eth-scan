{
  "name": "eth-scan",
  "version": "1.0.0",
  "description": "EVM balance scanner contract",
  "keywords": [
    "ethereum",
    "ether",
    "tokens",
    "balance",
    "scanner",
    "smart-contract"
  ],
  "repository": "https://github.com/brave-experiments/eth-scan.git",
  "author": "Brave Software",
  "contributors": [
    "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
    "Luit Hollander <luit@hollander.email> (https://luit.me)",
    "Anirudha Bose <anirudha.bose@alumni.cern>"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn compile",
    "compile": "hardhat compile",
    "test": "jest",
    "lint": "yarn lint:solidity && yarn lint:format",
    "lint:solidity": "hardhat check",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{ts,tsx,js,jsx,json,yml,sol}'",
    "estimate-gas": "ts-node --files scripts/estimate-gas.ts"
  },
  "dependencies": {
    "@findeth/abi": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.9.6",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^12.0.3",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.7.2",
    "hardhat": "^2.0.7",
    "hardhat-typechain": "^0.3.4",
    "husky": "^4.3.0",
    "jest": "^29.7.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "solhint": "^3.3.4",
    "ts-node": "^10.9.2",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{sol,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
